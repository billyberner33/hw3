<h1>Add a New Place</h1>

<%= form_with(model: @place, url: "/places", local: true) do |form| %>
  <p>
    <%= form.label :name, "Place Name" %><br>
    <%= form.text_field :name, name: "name", required: true %>
  </p>

  <p>
    <%= form.submit "Save Place" %>
  </p>
<% end %>

<%= link_to "Back to Places", "/places" %>
